CREATE TABLE IF NOT EXISTS `iqraa-12.diwan_iqraa_elmi.bibliographic_entries`
(
    entry_id STRING NOT NULL,

    title STRING,
    author STRING,
    editor STRING,
    publication_year INT64,
    publication_place STRING,
    publisher STRING,
    edition_info STRING,

    classification STRUCT<
        category STRING,
        subcategory STRING,
        tags ARRAY<STRING>
    >,

    related_persons ARRAY<
        STRUCT<
            person_id STRING,
            role STRING
        >
    >,

    related_texts ARRAY<
        STRUCT<
            text_id STRING,
            relation_type STRING
        >
    >,

    metadata STRUCT<
        sources ARRAY<STRING>,
        notes STRING,
        created_at TIMESTAMP
    >
)
CLUSTER BY publication_year, classification.category;


