        STRUCT<
            reason STRING,
            censor STRING,
            erased_passages ARRAY<STRING>,
            manuscript_comparisons ARRAY<STRING>
        >
    >,

    metadata STRUCT<
        created_at TIMESTAMP,
        sources ARRAY<STRING>
    >
)
CLUSTER BY city, century;
