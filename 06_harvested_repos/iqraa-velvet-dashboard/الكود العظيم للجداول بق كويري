-- ==========================================================
-- [028] historical_epochs_and_civilizational_phases
-- ==========================================================

CREATE TABLE IF NOT EXISTS `iqraa-12.diwan_iqraa_elmi.historical_epochs_and_civilizational_phases`
(
    epoch_id STRING NOT NULL,

    name STRING,
    start_year INT64,
    end_year INT64,

    defining_features ARRAY<
        STRUCT<
            feature STRING,
            description STRING
        >
    >,

    key_figures ARRAY<
        STRUCT<
            person_id STRING,
            role STRING
