-- ==========================================================
-- [001] agent_behavioral_rules
-- ==========================================================

CREATE TABLE IF NOT EXISTS `iqraa-12.diwan_iqraa_elmi.agent_behavioral_rules`
(
    rule_id STRING NOT NULL,

    agent_type STRING,

    decision_context STRUCT<
        context_name STRING
    >,

    state_variables ARRAY<
        STRUCT<
            variable_name STRING,
            variable_type STRING,
            current_value_range STRING
        >
    >,

    possible_actions ARRAY<STRING>,

    decision_rule STRUCT<
        rule_type STRING,
        condition_action_pairs ARRAY<
            STRUCT<
                condition STRING,
                action STRING,
                probability FLOAT64,
                utility_function STRING,
                parameters JSON
            >
        >,
        noise_level FLOAT64,
        is_adaptive BOOLEAN,
        adaptation_rule STRING
    >,

    calibration STRUCT<
        calibrated_from ARRAY<
            STRUCT<
                historical_case_id STRING,
                case_description STRING,
                fit_quality FLOAT64
            >
        >,
        validation_cases ARRAY<STRING>,
        goodness_of_fit STRUCT<
            r_squared FLOAT64,
            prediction_accuracy FLOAT64,
            confidence_interval STRING
        >
    >,

    contextual_modifiers ARRAY<
        STRUCT<
            modifier_type STRING,
            modifier_value STRING,
            effect_on_rule STRING
        >
    >,

    metadata STRUCT<
        created_at TIMESTAMP,
        evidence_base STRING,
        confidence_level STRING
    >
)
CLUSTER BY agent_type;


-- ==========================================================
-- [002] alternative_trajectories_counterfactuals
-- ==========================================================

CREATE TABLE IF NOT EXISTS `iqraa-12.diwan_iqraa_elmi.alternative_trajectories_counterfactuals`
(
    scenario_id STRING NOT NULL,

    divergence_point STRUCT<
        divergence_date DATE,
        divergence_century_hijri STRING
    >,

    actual_event STRUCT<
        event_id STRING,
        event_description STRING
    >,

    counterfactual_event STRUCT<
        alternative_description STRING,
        plausibility STRING,
        why_didnt_happen ARRAY<
            STRUCT<
                reason_type STRING,
                reason_description STRING
            >
        >
    >,

    alternative_scenario STRUCT<
        scenario_name STRING,
        scenario_description STRING,
        logical_chain ARRAY<
            STRUCT<
                step_number INT64,
                step_description STRING,
                probability_assessment STRING,
                assumptions ARRAY<STRING>
            >
        >,
        overall_plausibility STRUCT<
            plausibility_score FLOAT64,
            assessment_rationale STRING,
            supporting_factors ARRAY<STRING>,
            opposing_factors ARRAY<STRING>
        >
    >,

    projected_outcomes STRUCT<
        short_term ARRAY<
            STRUCT<
                domain STRING,
                projected_change STRING,
                confidence STRING
            >
        >,
        medium_term ARRAY<
            STRUCT<
                domain STRING,
                projected_change STRING,
                confidence STRING
            >
        >,
        long_term ARRAY<
            STRUCT<
                domain STRING,
                projected_change STRING,
                confidence STRING
            >
        >
    >,

    intellectual_trajectory_impact STRUCT<
        enabled_ideas ARRAY<
            STRUCT<
                idea_description STRING,
                likelihood STRING,
                potential_impact STRING
            >
        >,
        suppressed_ideas ARRAY<
            STRUCT<
                idea_description STRING,
                likelihood STRING
            >
        >,
        affected_schools ARRAY<
            STRUCT<
                school_id STRING,
                projected_fate STRING
            >
        >
    >,

    actual_vs_counterfactual STRUCT<
        key_differences ARRAY<
            STRUCT<
                dimension STRING,
                actual STRING,
                counterfactual STRING,
                significance STRING
            >
        >,
        insights_gained ARRAY<STRING>
    >,

    supporting_evidence STRUCT<
        analogous_cases ARRAY<
            STRUCT<
                case_description STRING,
                similarity STRING,
                outcome STRING
            >
        >,
        simulation_results STRUCT<
            simulation_method STRING,
            simulation_outcome STRING,
            confidence STRING
        >
    >,

    explanatory_value STRUCT<
        clarifies ARRAY<STRING>,
        debunked_inevitabilities ARRAY<STRING>,
        revealed_forces ARRAY<STRING>
    >,

    metadata STRUCT<
        scenario_author STRING,
        peer_review_status STRING,
        criticism ARRAY<STRING>,
        revision_history JSON
    >)
CLUSTER BY scenario_id;


-- ==========================================================
-- [001] agent_behavioral_rules
-- ==========================================================

CREATE TABLE IF NOT EXISTS `iqraa-12.diwan_iqraa_elmi.agent_behavioral_rules`
(
    rule_id STRING NOT NULL,

    agent_type STRING,

    decision_context STRUCT<
        context_name STRING
    >,

    state_variables ARRAY<
        STRUCT<
            variable_name STRING,
            variable_type STRING,
            current_value_range STRING
        >
    >,

    possible_actions ARRAY<STRING>,

    decision_rule STRUCT<
        rule_type STRING,
        condition_action_pairs ARRAY<
            STRUCT<
                condition STRING,
                action STRING,
                probability FLOAT64,
                utility_function STRING,
                parameters JSON
            >
        >,
        noise_level FLOAT64,
        is_adaptive BOOLEAN,
        adaptation_rule STRING
    >,

    calibration STRUCT<
        calibrated_from ARRAY<
            STRUCT<
                historical_case_id STRING,
                case_description STRING,
                fit_quality FLOAT64
            >
        >,
        validation_cases ARRAY<STRING>,
        goodness_of_fit STRUCT<
            r_squared FLOAT64,
            prediction_accuracy FLOAT64,
            confidence_interval STRING
        >
    >,

    contextual_modifiers ARRAY<
        STRUCT<
            modifier_type STRING,
            modifier_value STRING,
            effect_on_rule STRING
        >
    >,

    metadata STRUCT<
        created_at TIMESTAMP,
        evidence_base STRING,
        confidence_level STRING
    >
)
CLUSTER BY agent_type;


-- ==========================================================
-- [002] alternative_trajectories_counterfactuals
-- ==========================================================

CREATE TABLE IF NOT EXISTS `iqraa-12.diwan_iqraa_elmi.alternative_trajectories_counterfactuals`
(
    scenario_id STRING NOT NULL,

    divergence_point STRUCT<
        divergence_date DATE,
        divergence_century_hijri STRING
    >,

    actual_event STRUCT<
        event_id STRING,
        event_description STRING
    >,

    counterfactual_event STRUCT<
        alternative_description STRING,
        plausibility STRING,
        why_didnt_happen ARRAY<
            STRUCT<
                reason_type STRING,
                reason_description STRING
            >
        >
    >,

    alternative_scenario STRUCT<
        scenario_name STRING,
        scenario_description STRING,
        logical_chain ARRAY<
            STRUCT<
                step_number INT64,
                step_description STRING,
                probability_assessment STRING,
                assumptions ARRAY<STRING>
            >
        >,
        overall_plausibility STRUCT<
            plausibility_score FLOAT64,
            assessment_rationale STRING,
            supporting_factors ARRAY<STRING>,
            opposing_factors ARRAY<STRING>
        >
    >,

    projected_outcomes STRUCT<
        short_term ARRAY<
            STRUCT<
                domain STRING,
                projected_change STRING,
                confidence STRING
            >
        >,
        medium_term ARRAY<
            STRUCT<
                domain STRING,
                projected_change STRING,
                confidence STRING
            >
        >,
        long_term ARRAY<
            STRUCT<
                domain STRING,
                projected_change STRING,
                confidence STRING
            >
        >
    >,

    intellectual_trajectory_impact STRUCT<
        enabled_ideas ARRAY<
            STRUCT<
                idea_description STRING,
                likelihood STRING,
                potential_impact STRING
            >
        >,
        suppressed_ideas ARRAY<
            STRUCT<
                idea_description STRING,
                likelihood STRING
            >
        >,
        affected_schools ARRAY<
            STRUCT<
                school_id STRING,
                projected_fate STRING
            >
        >
    >,

    actual_vs_counterfactual STRUCT<
        key_differences ARRAY<
            STRUCT<
                dimension STRING,
                actual STRING,
                counterfactual STRING,
                significance STRING
            >
        >,
        insights_gained ARRAY<STRING>
    >,

    supporting_evidence STRUCT<
        analogous_cases ARRAY<
            STRUCT<
                case_description STRING,
                similarity STRING,
                outcome STRING
            >
        >,
        simulation_results STRUCT<
            simulation_method STRING,
            simulation_outcome STRING,
            confidence STRING
        >
    >,

    explanatory_value STRUCT<
        clarifies ARRAY<STRING>,
        debunked_inevitabilities ARRAY<STRING>,
        revealed_forces ARRAY<STRING>
    >,

    metadata STRUCT<
        scenario_author STRING,
        peer_review_status STRING,
        criticism ARRAY<STRING>,
        revision_history JSON
    >
)
CLUSTER BY
    divergence_point.divergence_century_hijri,
    alternative_scenario.overall_plausibility.plausibility_score;
