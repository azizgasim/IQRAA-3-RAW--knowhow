    logic_type STRING
);


-- ==========================================================
-- [004] arts_architecture_and_aesthetics
-- ==========================================================

CREATE TABLE IF NOT EXISTS `iqraa-12.diwan_iqraa_elmi.arts_architecture_and_aesthetics`
(
    artifact_id STRING,
    name_ar STRING,
    name_en STRING,

    category STRING,
    subcategory STRING,

    creation_year INT64,

    location_geo GEOGRAPHY,

    location_details STRUCT<
        city STRING,
        region STRING,
        modern_country STRING
    >,

    patronage_info STRUCT<
        patron_name STRING,
        patron_role STRING,
        political_affiliation STRING
    >,

    influences STRUCT<
        artistic_schools ARRAY<STRING>,
        cultural_layers ARRAY<STRING>
    >,

    materials ARRAY<STRING>,

    aesthetics STRUCT<
        dominant_style STRING,
        decorative_elements ARRAY<STRING>
    >,

    significance STRUCT<
        historical_value STRING,
        artistic_value STRING,
        cultural_value STRING
    >,

    metadata STRUCT<
        sources ARRAY<STRING>,
        notes STRING,
        created_at TIMESTAMP
    >
)
CLUSTER BY creation_year, category;


-- ==========================================================
-- [005] belief_types_and_classifications
-- ==========================================================

CREATE TABLE IF NOT EXISTS `iqraa-12.diwan_iqraa_elmi.belief_types_and_classifications`
(
    belief_id STRING NOT NULL,

    belief_name STRING,
    belief_category STRING,

    detailed_structure STRUCT<
        components ARRAY<
            STRUCT<
                component_name STRING,
                component_description STRING
            >
        >,
        dependencies ARRAY<
            STRUCT<
                dependency_type STRING,
                dependency_target STRING
            >
        >
    >,

    modern_comparisons ARRAY<
        STRUCT<
            comparison_domain STRING,
            similarity_note STRING,
            difference_note STRING
        >
    >,

    historical_notes ARRAY<
        STRUCT<
            period STRING,
            evolution_note STRING
        >
    >,

    metadata STRUCT<
        sources ARRAY<STRING>,
        created_at TIMESTAMP,
        reliability STRING
    >
)
CLUSTER BY belief_category;
