    region STRING,

    volumes INT64,
    manuscripts ARRAY<
        STRUCT<
            library STRING,
            shelfmark STRING,
            notes STRING
        >
    >,

    indexing STRUCT<
        main_topics ARRAY<STRING>,
        keywords ARRAY<STRING>,
        volume_index JSON
    >,

    transmission STRUCT<
        narrators ARRAY<
            STRUCT<
                name STRING,
                reliability STRING,
                isnad STRING
            >
        >,
        grading STRING,
        notes STRING
    >,

    metadata STRUCT<
        created_at TIMESTAMP,
        sources ARRAY<STRING>,
        reliability STRING
    >
)
CLUSTER BY century;


